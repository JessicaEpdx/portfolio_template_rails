<%= render 'layouts/errors', :object => object %>

<%= form_for object do |f| %>
  <div class="form-group">
    <%= f.text_field :name, :placeholder => "Skill Name" %>
  </div>
  <div class="form-group">
    <%= f.text_field :description, :placeholder => "Description" %>
  </div>
  <%= f.submit :class => 'btn btn-warning' %>
<% end %>
